{
  "name": "progress-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r 'babel-register'\" && npm run open:dev",
    "open:dev": "babel-node scripts/open-dev",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && mkdirp dist",
    "build": "babel src --out-dir dist --ignore spec.js",
    "test": "./node_modules/.bin/mocha test-configuration.js  \"./{,!(node_modules)/**/}*.spec.js\" --reporter list --require babel-polyfill",
    "test:watch": "npm run test -- --watch",
    "lint": "./node_modules/.bin/esw src",
    "lint:watch": "esw -w src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "commander": "^2.9.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.14.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "fs-extra": "^2.0.0",
    "fs.extra": "^1.3.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "open": "^0.0.5",
    "rimraf": "^2.5.4",
    "supertest": "^2.0.1",
    "testdouble": "^1.11.1"
  }
}
