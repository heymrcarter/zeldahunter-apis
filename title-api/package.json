  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "yarn run build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r 'babel-register'\" && yarn run open:dev",
    "open:dev": "babel-node scripts/open-dev",
    "clean": "rm -rf dist",
    "prebuild": "yarn run clean && mkdir -p dist",
    "build": "babel src -s -D -d",
    "test": "mocha scripts/test-setup.js  \"./{,!(node_modules)/**/}*.spec.js\" --reporter list --require babel-polyfill",
    "test:watch": "yarn run test -- --watch",
    "lint": "esw -w src"
  },
  "keywords": [],
