  "license": "ISC",
  "dependencies": {
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.14.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "open": "^0.0.5",
    "supertest": "^2.0.1"
  }
